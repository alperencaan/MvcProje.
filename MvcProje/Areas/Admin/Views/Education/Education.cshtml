@model Model.ViewModels.AuthenticationViewModel

@{
    ViewData["Title"] = "Index";
}

<div class="container-fluid">
    <h2>Eğitim Listesi</h2>
    <table class="table table-bordered">
        <tr>
            <th>ID</th>
            <th>Başlık</th>
            <th>Alt Başlık</th>
            <th>Alt Başlık 2</th>
            <th>GNO</th>
            <th>Tarih</th>
        </tr>
        @foreach (var item in Model.Educations)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.Subtitle</td>
                <td>@item.Subtitle2</td>
                <td>@item.Gno</td>
                <td>@item.Date</td>
            </tr>
        }
    </table>
</div>